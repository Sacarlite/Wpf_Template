<Window x:Class="Vievs.Windows.AboutWindow.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:aboutWindow="clr-namespace:VievModels.VievModels.AboutWindowVievModel;assembly=VievModel"
        xmlns:local="clr-namespace:Vievs.Windows.AboutWindow"
        xmlns:converters="clr-namespace:Vievs.Converters"
        mc:Ignorable="d"
        WindowState= "Normal"
        ResizeMode="NoResize"
        WindowStyle="SingleBorderWindow"
        Title="{Binding Title, Mode=OneTime}"
        Height="{Binding Height, Mode=TwoWay}"
        Width="{Binding Width,  Mode=TwoWay}"
        Left="{Binding Left,  Mode=TwoWay}"
        Top="{Binding Top,  Mode=TwoWay}"
        d:DataContext="{d:DesignInstance aboutWindow:AboutVievModel}">
    <Grid>
        
    </Grid>
</Window>
